urxvt*foreground: white
#urxvt*background: black
#URxvt*.transparent: true
URxvt*.shading: 20
URxvt.scrollstyle: plain
URxvt.perl-ext-common: clipboard
URxvt.depth: 32
URxvt.background: rgba:1111/1111/1111/dddd

*color0:  #2E3436
*color1:  #a40000
*color2:  #4E9A06
*color3:  #C4A000
*color4:  #3465A4
*color5:  #75507B
*color6:  #ce5c00
*color7:  #babdb9
*color8:  #555753
*color9:  #EF2929
*color10: #8AE234
*color11: #FCE94F
*color12: #729FCF
*color13: #AD7FA8
*color14: #fcaf3e
*color15: #EEEEEC

! Fonts
URxvt.allow_bold:           false
!URxvt.font:                 -*-terminus-medium-r-normal-*-12-120-72-72-c-60-iso8859-1
! URxvt*font: xft:Anonymous Pro:pixelsize=14:antialiasing=true:hinting=true
!URxvt*font:       xft:Anonymous Pro:pixelsize=14:antialiasing=true
URxvt*font:       xft:DejaVu Sans Mono:pixelsize=14:antialiasing=true
! URxvt*boldFont: xft:Anonymous Pro:pixelsize=14:antialiasing=true:hinting=true

! Fix font space
URxvt*letterSpace: 0
